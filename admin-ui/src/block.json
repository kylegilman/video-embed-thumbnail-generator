{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "videopack/videopack-block",
	"version": "1.0.0",
	"title": "Videopack Video",
	"category": "media",
	"icon": "format-video",
	"description": "Embed videos with advanced options, thumbnail generation, and galleries.",
	"supports": {
		"html": false,
		"align": [ "left", "right", "center", "wide", "full" ]
	},
	"textdomain": "video-embed-thumbnail-generator",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"id": {
			"type": "number"
		},
		"src": {
			"type": "string"
		},
		"videoTitle": {
			"type": "string"
		},
		"poster": {
			"type": "string"
		},
		"poster_id": {
			"type": "number"
		},
		"width": {
			"type": "number"
		},
		"height": {
			"type": "number"
		},
		"align": {
			"type": "string",
			"default": "none"
		},
		"fullwidth": {
			"type": "boolean"
		},
		"fixed_aspect": {
			"type": "string",
			"default": "vertical"
		},
		"controls": {
			"type": "boolean"
		},
		"loop": {
			"type": "boolean"
		},
		"autoplay": {
			"type": "boolean"
		},
		"muted": {
			"type": "boolean"
		},
		"playsinline": {
			"type": "boolean"
		},
		"gifmode": {
			"type": "boolean"
		},
		"pauseothervideos": {
			"type": "boolean"
		},
		"preload": {
			"type": "string",
			"default": "metadata"
		},
		"start": {
			"type": "string"
		},
		"skin": {
			"type": "string"
		},
		"title": {
			"type": "boolean"
		},
		"caption": {
			"type": "string"
		},
		"description": {
			"type": "string"
		},
		"downloadlink": {
			"type": "boolean"
		},
		"right_click": {
			"type": "boolean"
		},
		"auto_res": {
			"type": "string",
			"default": "automatic"
		},
		"pixel_ratio": {
			"type": "boolean"
		},
		"schema": {
			"type": "boolean"
		},
		"gallery": {
			"type": "boolean",
			"default": false
		},
		"gallery_columns": {
			"type": "number"
		},
		"gallery_orderby": {
			"type": "string",
			"default": "menu_order ID"
		},
		"gallery_order": {
			"type": "string",
			"default": "ASC"
		},
		"gallery_include": {
			"type": "string"
		},
		"gallery_exclude": {
			"type": "string"
		},
		"gallery_id": {
			"type": "number"
		},
		"gallery_pagination": {
			"type": "boolean"
		},
		"gallery_per_page": {
			"type": "number"
		},
		"gallery_title": {
			"type": "boolean"
		}
	}
}
